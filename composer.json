{
    "name": "dahovitech/smart-crud-bundle",
    "description": "A Symfony bundle to simplify CRUD operations with advanced features like translations and user management.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Dahovi",
            "email": "contact@dahovi.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/framework-bundle": "^6.0 || ^7.0",
        "doctrine/orm": "^2.10",
        "gedmo/doctrine-extensions": "^3.10",
        "symfony/translation": "^6.0 || ^7.0",
        "symfony/security-bundle": "^6.0 || ^7.0",
        "symfony/form": "^6.0 || ^7.0",
        "symfony/validator": "^6.0 || ^7.0",
        "symfony/twig-bundle": "^6.0 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "Dahovitech\\SmartCrudBundle\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}